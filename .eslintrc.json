{
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "eslint-plugin-import/prefer-default-export": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}